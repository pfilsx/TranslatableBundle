{
    "name": "pfilsx/translatable-bundle",
    "description": "Provides translatable behavior to Doctrine entities",
    "type": "symfony-bundle",
    "keywords": [
        "doctrine", "behavior", "translatable"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Filimonov",
            "email": "pfilsx@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.7",
        "symfony/dependency-injection": "^4.3|^5.0",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/yaml": "^4.3|^5.0",
        "symfony/config": "^4.3|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpunit/php-code-coverage": "^7.0",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "^4.3|^5.0",
        "symfony/orm-pack": "*"
    },
    "autoload": {
        "psr-4": {
            "Pfilsx\\Translatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pfilsx\\Translatable\\Tests\\": "tests"
        }
    }
}
